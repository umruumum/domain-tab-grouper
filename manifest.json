{
  "manifest_version": 3,
  "name": "Domain Tab Grouper",
  "version": "0.1.0",
  "description": "ドメインベースでタブを自動グループ化するChrome拡張機能",
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Domain Tab Grouper"
  }
}